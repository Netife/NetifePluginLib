{
  "name":"NetifePluginLib",
  "clsid":"2f81e161-7124-420e-9397-1db3cb969b0b",
  "author":"EpicMo",
  "version":"1.0.0",
  "exportWay":"cpp",
  "description":"本插件为 Netife 内置核心插件库，为其他插件提供依赖支持，请勿移除本插件。",
  "coreRelative":"v1",
  "exportCommand":"Netife.CppExport()",
  "exportClassName":"NetifePluginLib",
  "relativeChains":[],
  "registerCommand":[
    {
      "class":"NetifeBasicLib",
      "command":"parserRawHtml [rawHtml]",
      "description":"将 rawHtml 以人类可读的 JSON 方式返回"
    },
    {
      "class":"NetifeBasicLib",
      "command":"parserHexString [rawHtml] <targetType>",
      "description":"将 HexString 以指定的格式返回。默认为转换成 String，targetType可为 'string'(字符串), 'binary'(二进制字符串)。"
    },
    {
      "class": "NetifeBasicLib",
      "command": "downloadFileStream [UUID] <UUID_SUB>",
      "description": "将指定 UUID 文件载体写入文件，并返回地址"
    }
  ],
  "registerHook":[
    {
      "node":"netife.network.receive",
      "symbol":"LoadMe"
    }
  ]
}